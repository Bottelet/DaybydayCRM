{
    "name": "bottelet/daybyday-crm",
    "description": "DaybydayCRM is a free, open-source and self-hosted CRM platform based of Laravel",
    "keywords": [
        "laravel",
        "CRM",
        "customer management",
        "Lead management",
        "customer relationship management"
    ],
    "authors": [
        {
            "name": "Casper Bottelet",
            "email": "cbottelet@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.112",
        "elasticsearch/elasticsearch": "^8.15.0",
        "google/apiclient": "^2.2",
        "laravel/cashier": "*",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^1.1",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^v4.5.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mews/purifier": "^3.1",
        "predis/predis": "^v2.2.2",
        "ramsey/uuid": "^4.7.6",
        "spatie/dropbox-api": "^1.19",
        "yajra/laravel-datatables-oracle": "^v11.1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~3.2",
        "laravel/dusk": "^8.2.10",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1",
        "symfony/css-selector": "^7.1.6",
        "symfony/dom-crawler": "^7.1.6"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
