{
    "name": "bottelet/daybyday-crm",
    "description": "DaybydayCRM is a free, open-source and self-hosted CRM platform based of Laravel",
    "keywords": [
        "laravel",
        "CRM",
        "customer management",
        "Lead management",
        "customer relationship management"
    ],
    "authors": [
        {
            "name": "Casper Bottelet",
            "email": "cbottelet@gmail.com"
        }
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.112",
        "elasticsearch/elasticsearch": "^6.1",
        "google/apiclient": "^2.2",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^1.1",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^v4.5.2",
        "league/flysystem-aws-s3-v3": "^3.0",
        "mews/purifier": "^3.1",
        "predis/predis": "^1.1",
        "ramsey/uuid": "4.7.6",
        "spatie/dropbox-api": "^1.19",
        "yajra/laravel-datatables-oracle": "^v11.1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "~3.2",
        "laravel/dusk": "^8.2.10",
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.0.1",
        "symfony/css-selector": "2.8.*|3.0.*",
        "symfony/dom-crawler": "2.8.*|3.0.*"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": [
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-install-cmd": [
            "php artisan route:cache"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
